# Environment variables for the OSINT AI Worker

# LLM API Keys
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here
OPENROUTER_API_KEY=your_openrouter_api_key_here

# Search Services
TAVILY_API_KEY=your_tavily_api_key_here

# Langfuse Configuration for Performance Tracking
LANGFUSE_SECRET_KEY=your_langfuse_secret_key_here
LANGFUSE_PUBLIC_KEY=your_langfuse_public_key_here
LANGFUSE_HOST=https://cloud.langfuse.com

# Langfuse Control Options (Optional)
LANGFUSE_ENABLED=true
LANGFUSE_DEBUG=false

# Optional: Custom Langfuse Instance
# LANGFUSE_HOST=https://your-custom-langfuse-instance.com
